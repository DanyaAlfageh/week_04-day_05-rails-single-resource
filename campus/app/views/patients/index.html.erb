<h1>Patient</h1>

<p> <%= link_to "Add a Patient", new_patient_path %> </p>
<ul>
<% @patients.each do |patient| %>
    
    <li>
        <%= link_to patient.first_name, patient_path(patient) %>
        <%= link_to patient.last_name, patient_path(patient) %>
        <%=patient.born_on%>
    </li>
    
<% end %>
</ul>